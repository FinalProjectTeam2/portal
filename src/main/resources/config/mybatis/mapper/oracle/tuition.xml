<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.tuition">
	<select id="selectStu" parameterType="string" resultType="TuitionStuVO">
		select * from student_view2
	</select>
   
   	<select id="tuition" parameterType="string" resultType="TuitionStuVO">
		select * from tuition_view
	</select>
   
   	<select id="tuitionD" parameterType="string" resultType="TuitionStuVO">
		select * from tuitionD_view
	</select>
	
	
	<resultMap type="TuitionStuVO" id="tuitionResultMap">
		<association property="tuitionVo" javaType="tuitionVo" column="stuNo">
			<id column="stuNo" property="stuNo" jdbcType="VARCHART"/>
			<result column="deposit_state" property="deposit_state" jdbcType="VARCHAR"/>
			<result column="deposit_date" property="deposit_date" jdbcType="TIMESTAMP"/>
			<result column="tuition" property="tuition" jdbcType="BIGINT"/>
			<result column="admissionfee" property="admissionfee" jdbcType="BIGINT"/>
			<result column="practicecost" property="practicecost" jdbcType="BIGINT"/>
			<result column="studentfee" property="studentfee" jdbcType="BIGINT"/>
			<result column="total" property="total" jdbcType="BIGINT"/>
		</association>
		<collection property="" column="stuNo"
			javaType="ArrayList" 
			ofType="map" select="">		
		</collection>
	</resultMap>
</mapper>











