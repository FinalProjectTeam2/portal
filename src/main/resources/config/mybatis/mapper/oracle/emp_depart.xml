<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.emp_depart">
	<select id="selectEmpDepart" resultType="emp_departVO">
		select * from emp_depart
	</select>
   <select id="selectEmpDepartByCode" parameterType="String" resultType="emp_departVO">
		select * from emp_depart where dep_code = #{depCode}
	</select>
</mapper>











